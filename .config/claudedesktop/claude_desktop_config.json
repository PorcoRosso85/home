{
  "mcpServers": {
    "claude_code": {
      "command": "wsl",
      "args": [
        "bash",
        "-c",
        "pnpx @anthropic-ai/claude-code mcp serve"
      ]
    },
    "excel": {
      "command": "wsl",
      "args": [
        "bash",
        "-c",
        "pnpx @negokaz/excel-mcp-server"
      ],
      "env": {
        "EXCEL_MCP_PAGING_CELLS_LIMIT": "4000"
      }
    },
    "llm_functions": {
      "command": "wsl",
      "args": [
        "bash",
        "-c",
        "pnpx mcp-llm-functions /home/nixos/bin/src"
      ]
    }
  }
}
