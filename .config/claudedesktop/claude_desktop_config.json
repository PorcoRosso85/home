{
  "mcpServers": {
    "claude_code": {
      "command": "wsl",
      "args": [
        "bash",
        "-c",
        "ALLOWED_TOOLS='[\"GlobTool\", \"Edit\", \"GrepTool\", \"dispatch_agent\", \"LS\", \"Replace\", \"View\"]' claude mcp serve"
      ]
    },
    "desktop_commander": {
      "command": "wsl",
      "args": [
        "bash",
        "-c",
        "pnpx @wonderwhy-er/desktop-commander"
      ]
    },
    "excel": {
      "command": "wsl",
      "args": [
        "bash",
        "-c",
        "pnpx @negokaz/excel-mcp-server"
      ],
      "env": {
        "EXCEL_MCP_PAGING_CELLS_LIMIT": "4000"
      }
    },
    "llm_functions": {
      "command": "wsl",
      "args": [
        "bash",
        "-c",
        "pnpx mcp-llm-functions /home/nixos/bin/src"
      ]
    }
  }
}
