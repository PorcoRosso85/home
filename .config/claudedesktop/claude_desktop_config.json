{
  "mcpServers": {
    "claude_code": {
      "command": "wsl",
      "args": [
        "claude",
        "mcp",
        "serve"
      ],
      "env": {}
    },
    "llm_functions": {
      "command": "wsl",
      "args": [
        "bash",
        "-c",
        "pnpx mcp-llm-functions /home/nixos/bin/src/aichat_extension"
      ]
    }
  }
}
