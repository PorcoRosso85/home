{
  "mcpServers": {
    "execute_shell": {
      "command": "wsl",
      "args": [
        "bash",
        "-c",
        "pnpx mcp-shell"
      ]
    },
    "filesystem": {
      "command": "wsl",
      "args": [
        "bash",
        "-c",
        "pnpx @modelcontextprotocol/server-filesystem /home/nixos"
      ]
    }
  }
}
