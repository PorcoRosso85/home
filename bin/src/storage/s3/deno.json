{
  "tasks": {
    "test": "deno test --allow-all",
    "fmt": "deno fmt",
    "lint": "deno lint",
    "dev": "deno run --allow-all --watch main.ts"
  },
  "imports": {
    "@aws-sdk/client-s3": "npm:@aws-sdk/client-s3@^3.0.0"
  },
  "fmt": {
    "include": ["*.ts", "*.md"],
    "lineWidth": 100,
    "indentWidth": 2,
    "singleQuote": true
  },
  "lint": {
    "include": ["*.ts"],
    "rules": {
      "tags": ["recommended"]
    }
  }
}