{
  "id": "api-gateway",
  "path": "/test_modules/api",
  "purpose": "API gateway for routing and authentication",
  "type": "gateway",
  "version": "1.2.0",
  "dependencies": [
    {"id": "auth-service", "version": ">=1.0.0"},
    {"id": "database-layer", "version": ">=1.0.0"}
  ],
  "interfaces": {
    "inputs": [
      {
        "name": "request",
        "type": "http",
        "description": "HTTP request"
      }
    ],
    "outputs": [
      {
        "name": "response",
        "type": "http",
        "description": "HTTP response"
      }
    ]
  },
  "responsibilities": [
    "Route requests to services",
    "Validate authentication",
    "Rate limiting"
  ],
  "constraints": {
    "performance": "< 50ms latency",
    "availability": "99.9%"
  }
}