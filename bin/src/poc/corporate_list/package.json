{
  "name": "corporate-list-scraper",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "scrape": "node scripts/switchover.mjs",
    "scrape:legacy": "node scrape.mjs",
    "scrape:ts": "tsx src/main.ts",
    "test": "node --test test/golden-master.test.js",
    "test:golden": "node --test test/golden-master.test.js",
    "test:validate": "node test/validate-golden.js",
    "test:ts": "node --test test/typescript-env.test.ts",
    "test:main": "node --test test/main.test.ts",
    "test:migration": "tsx --test test/migration.test.ts",
    "test:switchover": "node --test test/switchover-simple.test.js",
    "test:all": "npm run test && npm run test:ts && npm run test:main && npm run test:switchover",
    "build": "tsc",
    "dev": "tsx"
  },
  "dependencies": {
    "playwright-core": "^1.49.1"
  },
  "devDependencies": {
    "@types/node": "^22.11.6",
    "typescript": "^5.7.2",
    "tsx": "^4.19.2"
  }
}