{
    "ApiService": {
        "namespace": "corp.example",
        "name": "user-api",
        "role": "service",
        "version": "3.4.1",
        "dependsOn": [
            {
                "kind": "db",
                "target": "corp.example/postgres-db",
                "id": "primary-db",
                "versionRange": "^1.0.0",
                "description": "User data storage"
            },
            {
                "kind": "cache",
                "target": "corp.example/redis-cache",
                "id": "primary-cache",
                "versionRange": "^2.0.0",
                "description": "Session and data caching",
                "optional": true
            }
        ],
        "description": "REST API service for user management",
        "provides": [
            {
                "kind": "http",
                "id": "user-api",
                "port": 8080,
                "protocol": "https",
                "scope": "public",
                "description": "User management REST API"
            }
        ],
        "tags": [
            "api",
            "users",
            "http",
            "rest"
        ]
    }
}
