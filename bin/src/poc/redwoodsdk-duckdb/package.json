{
  "name": "redwoodsdk-duckdb",
  "version": "1.0.0",
  "description": "Build fast, server-driven webapps on Cloudflare with SSR, RSC, and realtime. ",
  "main": "index.js",
  "type": "module",
  "keywords": [],
  "author": "",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "vite build",
    "dev": "vite dev",
    "dev:nixos": "wrangler dev --port 3000",
    "dev:vite-only": "vite dev --mode development",
    "dev:init": "rw-scripts dev-init",
    "preview": "vite preview",
    "worker:run": "rw-scripts worker-run",
    "clean": "npm run clean:vite",
    "clean:vite": "rm -rf ./node_modules/.vite",
    "release": "rw-scripts ensure-deploy-env && npm run clean && prisma generate && RWSDK_DEPLOY=1 npm run build && wrangler deploy",
    "migrate:dev": "prisma generate && wrangler d1 migrations apply DB --local",
    "migrate:prd": "wrangler d1 migrations apply DB --remote",
    "migrate:new": "rw-scripts migrate-new",
    "seed": "npm run worker:run ./src/scripts/seed.ts",
    "generate": "rw-scripts ensure-env && prisma generate && wrangler types",
    "check": "npm run generate && npm run types",
    "types": "tsc",
    "test": "vitest",
    "test:unit": "vitest run tests/unit",
    "check:assets": "test -d public/duckdb && test -f public/duckdb/duckdb-browser.mjs && test -f public/duckdb/duckdb-mvp.wasm && test -f public/duckdb/duckdb-browser-mvp.worker.js && echo '✅ All DuckDB assets available (mjs, wasm, worker)' || echo '❌ DuckDB assets missing - run: nix develop'",
    "test:integration": "vitest run tests/integration",
    "test:e2e": "vitest run tests/e2e",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "init": "bash scripts/init.sh",
    "deploy:check": "bash scripts/deploy-check.sh",
    "deploy:safe": "bash scripts/deploy-safe.sh",
    "rollback": "bash scripts/rollback.sh",
    "prepare": "vibe-rules install cursor",
    "postinstall": "[ -f ./patch-workerd.sh ] && ./patch-workerd.sh || true"
  },
  "dependencies": {
    "@prisma/adapter-d1": "~6.8.2",
    "@prisma/client": "~6.8.2",
    "@simplewebauthn/browser": "^13.1.0",
    "@simplewebauthn/server": "^13.1.1",
    "rwsdk": "0.1.39"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "1.7.4",
    "@cloudflare/workers-types": "^4.20250407.0",
    "@types/node": "^22.14.0",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitest/ui": "^1.2.0",
    "miniflare": "^3.20240129.0",
    "prisma": "~6.8.2",
    "typescript": "^5.8.3",
    "vibe-rules": "^0.2.31",
    "vite": "^6.2.6",
    "vitest": "^1.2.0",
    "wrangler": "^4.20.5"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@prisma/client",
      "@prisma/engines",
      "esbuild",
      "prisma",
      "sharp",
      "workerd"
    ]
  }
}
