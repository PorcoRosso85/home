{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$metaSchema": "StructTypeMetaSchema",
  "type": "object",
  "properties": {
    "userId": {
      "type": "string",
      "description": "ユーザーID",
      "pattern": "^[0-9a-f]{8}-[0-9a-f]{4}-4[0-9a-f]{3}-[89ab][0-9a-f]{3}-[0-9a-f]{12}$"
    },
    "username": {
      "type": "string",
      "description": "ユーザー名",
      "minLength": 3,
      "maxLength": 50
    },
    "email": {
      "type": "string",
      "description": "メールアドレス",
      "format": "email"
    },
    "fullName": {
      "type": "string",
      "description": "フルネーム"
    },
    "age": {
      "type": "integer",
      "description": "年齢",
      "minimum": 13
    },
    "address": {
      "type": "object",
      "description": "住所",
      "properties": {
        "street": {
          "type": "string"
        },
        "city": {
          "type": "string"
        },
        "postalCode": {
          "type": "string"
        },
        "country": {
          "type": "string"
        }
      }
    },
    "phoneNumber": {
      "type": "string",
      "description": "電話番号"
    },
    "createdAt": {
      "type": "string",
      "description": "アカウント作成日時",
      "format": "date-time"
    },
    "updatedAt": {
      "type": "string",
      "description": "最終更新日時",
      "format": "date-time"
    },
    "isActive": {
      "type": "boolean",
      "description": "アクティブ状態",
      "default": true
    }
  },
  "required": [
    "userId",
    "username",
    "email",
    "createdAt"
  ],
  "title": "User",
  "description": "ユーザー型の定義 (メタスキーマから生成)",
  "additionalProperties": false
}