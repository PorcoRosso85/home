{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Test Schema 3",
  "definitions": {
    "country": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string"
        },
        "code": {
          "type": "string",
          "pattern": "^[A-Z]{2}$"
        }
      },
      "required": ["name", "code"]
    }
  }
}
