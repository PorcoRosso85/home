{
  "nodeModulesDir": "auto",
  "imports": {
    "vite": "npm:vite@^4.0.0",
    "vite-plugin-terminal": "npm:vite-plugin-terminal@^0.1.0",
    "kuzu-wasm": "npm:kuzu-wasm@^0.8.0",
    // NOTE: WASMモジュールをESM統合するためのプラグイン。Viteデフォルトではブラウザ互換性のためにWASMモジュールが外部化されるため、このプラグインが必要
    "vite-plugin-wasm": "npm:vite-plugin-wasm@^3.4.1",
    // NOTE: モジュールトップレベルでのawait使用をサポートするプラグイン。WASMモジュールの非同期初期化に必要
    "vite-plugin-top-level-await": "npm:vite-plugin-top-level-await@^1.4.1"
  }
}