    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "hono": "4.9.4",
    "react": "19.1.1",
    "react-aria-components": "^1.12.0",
    "react-dom": "19.1.1",
    "react-server-dom-webpack": "19.1.1",
    "uplot": "^1.6.32",
    "waku": "0.26.0",
    "wrangler": "4.32.0"
  },
  "devDependencies": {
    "@types/node": "24.3.0",
    "@types/react": "19.1.10",
    "@types/react-dom": "19.1.7",
    "miniflare": "4.20250816.1",
    "typescript": "5.9.2",
    "vite": "7.1.3"
  }
